    "@capacitor/status-bar": "^6.0.1",
    "@capacitor/toast": "^6.0.2",
    "@sentry/react": "^10.22.0",
    "clsx": "^2.0.0",
    "date-fns": "^3.0.6",
    "idb": "^8.0.0",
    "lucide-react": "^0.294.0",
